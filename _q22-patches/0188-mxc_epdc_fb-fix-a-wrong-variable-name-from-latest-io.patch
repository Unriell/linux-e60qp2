@@ -3812,7 +3812,7 @@ static int mxc_epdc_fb_ioctl(struct fb_info *info, unsigned int cmd,
 				sizeof(upd_data))) {
 
 				// set upd_data <- upd_data_old
-				memcpy(&upd_data_new.update_region, &upd_data_old.update_region, sizeof(upd_data_old.update_region));
